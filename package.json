{
  "name": "redux-offline-example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@redux-offline/redux-offline": "2.5.1",
    "classnames": "2.2.6",
    "husky": "1.3.1",
    "json-server": "0.14.2",
    "lint-staged": "8.1.5",
    "lodash": "4.17.11",
    "prettier": "1.16.4",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "6.0.1",
    "react-scripts": "2.1.8",
    "redux": "4.0.1",
    "redux-actions": "2.6.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0",
    "uuid": "3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write --trailing-comma es5",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "start-server": "json-server --watch db.json --port 3001",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
